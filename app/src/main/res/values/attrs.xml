<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CalendarGrid">
        <attr name="lineWidth" format="dimension"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="lineColor" format="color"/>
        <attr name="dayColor" format="color"/>
        <attr name="weekendColor" format="color"/>
        <attr name="initialRooms" format="integer"/>
        <attr name="headerHeight" format="dimension"/>
        <attr name="headerMargin" format="dimension"/>
        <attr name="marginColor" format="color"/>
        <attr name="headerTextColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="CalendarHeader">
        <attr name="header_line_width" format="dimension"/>
        <attr name="header_Height" format="dimension"/>
        <attr name="header_weekendColor" format="color"/>
        <attr name="header_marginColor" format="color"/>
        <attr name="header_backgroundColor" format="color"/>
        <attr name="header_headerTextColor" format="color"/>
        <attr name="header_headerMargin" format="dimension"/>
        <attr name="header_lineColor" format="color"/>
    </declare-styleable>
</resources>